#!/bin/sh

set -e

case "$1" in
    configure)
    install-info --section "General commands" "General commands" --quiet /usr/share/info/mtools.info.gz
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
